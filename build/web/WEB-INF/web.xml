<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>reg</servlet-name>
        <servlet-class>reg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Doctor</servlet-name>
        <servlet-class>Doctor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>patient</servlet-name>
        <servlet-class>patient</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>symptom</servlet-name>
        <servlet-class>symptom</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>treatment</servlet-name>
        <servlet-class>treatment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewdoctor</servlet-name>
        <servlet-class>viewdoctor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewpatient</servlet-name>
        <servlet-class>viewpatient</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>reg</servlet-name>
        <url-pattern>/reg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Doctor</servlet-name>
        <url-pattern>/Doctor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>patient</servlet-name>
        <url-pattern>/patient</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>symptom</servlet-name>
        <url-pattern>/symptom</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>treatment</servlet-name>
        <url-pattern>/treatment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewdoctor</servlet-name>
        <url-pattern>/viewdoctor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewpatient</servlet-name>
        <url-pattern>/viewpatient</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
